{
  "name": "react-notify-resize",
  "version": "1.0.3",
  "description": "React Component that notifies you whenever it is resized",
  "main": "lib/index.js",
  "scripts": {
    "lib": "rm -fr lib && babel --out-dir lib src",
    "prepublish": "npm run build",
    "build": "npm run dist && npm run lib",
    "dist": "webpack -p --progress --colors --config ./build/dist.config.js",
    "dev": "webpack-dev-server --progress --colors --config ./build/dev.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zippyui/react-notify-resize.git"
  },
  "keywords": [
    "react",
    "react-component",
    "resize",
    "notify",
    "event",
    "resizer",
    "resizable"
  ],
  "author": "ZippyUI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zippyui/react-notify-resize/issues"
  },
  "homepage": "https://github.com/zippyui/react-notify-resize#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.3.21",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1",
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4"
  },
  "dependencies": {
    "react-class": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.3 || ^15.0.0",
    "react-dom": "^0.14.3 || ^15.0.0"
  }
}
